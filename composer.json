{
    "name": "absuite/suite-laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "suite","amb"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "league/oauth2-server": "^6.0",
        "phpseclib/phpseclib": "^2.0",
        "predis/predis": "^1.1",
        "maatwebsite/excel": "~2.1.0",
        "simplesoftwareio/simple-qrcode": "^1.5",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "6.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Gmf\\Sys\\":"../../gmf/gmf-sys/src/",
            "Gmf\\Ac\\":"../../gmf/gmf-ac/src/",
            "Gmf\\Passport\\":"../../gmf/gmf-passport/src/",
            "Suite\\Cbo\\":"../suite-cbo/src/",
            "Suite\\Amiba\\":"../suite-amiba/src/",
            "Suite\\Bec\\":"../suite-bec/src/",
            "Suite\\Docs\\":"../suite-docs/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}